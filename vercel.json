{
    "version": 2,
    "builds": [
        {
            "src": "src/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "status": 204
        },
        {
            "src": "/api/(.*)",
            "dest": "src/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "src/main.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "FLASK_ENV": "production"
    },
    "build": {
        "env": {
            "PIP_TARGET": ".pip"
        }
    }
}